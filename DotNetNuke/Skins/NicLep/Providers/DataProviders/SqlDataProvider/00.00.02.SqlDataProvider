
IF EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'{databaseOwner}[{objectQualifier}CKE_Settings]') AND type in (N'U'))
BEGIN
	
	UPDATE CKE_Settings SET SettingValue='../../portals/_default/skins/NicLep/Resources/js/ckeditor/templates/sd.fn.templates.js' WHERE SettingName='DNNCKP#0#templates_files'

END 
GO