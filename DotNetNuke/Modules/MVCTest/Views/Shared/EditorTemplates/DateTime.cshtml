@inherits  DotNetNuke.Web.Mvc.Framework.DnnWebViewPage<Nullable<DateTime>>

@using System.Web.Mvc

@Html.TextBox(""
    , Model.HasValue ? String.Format("{0}", Model.Value.ToString("dd/MM/yyyy HH:mm")) : string.Empty     
    , new { @class = "textboxdatetime", type = "datetime", autocomplete = "off" }
    )
